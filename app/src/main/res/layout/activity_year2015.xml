        android:layout_alignRight="@+id/edit_feb_2015"
        android:layout_alignEnd="@+id/edit_feb_2015" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="June"
        android:id="@+id/textView7"
        android:layout_above="@+id/edit_may_2015"
        android:layout_alignLeft="@+id/edit_march_2015"
        android:layout_alignStart="@+id/edit_march_2015" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/edit_june_2015"
        android:layout_alignBottom="@+id/edit_may_2015"
        android:layout_alignLeft="@+id/textView7"
        android:layout_alignStart="@+id/textView7" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="July"
        android:id="@+id/textView8"
