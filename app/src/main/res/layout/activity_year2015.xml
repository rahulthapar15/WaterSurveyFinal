    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="December"
        android:id="@+id/textView13"
        android:layout_above="@+id/edit_nov_2015"
        android:layout_alignLeft="@+id/edit_sept_2015"
        android:layout_alignStart="@+id/edit_sept_2015" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="phone"
        android:ems="10"
        android:id="@+id/edit_dec_2015"
        android:layout_alignBottom="@+id/edit_nov_2015"
        android:layout_alignLeft="@+id/textView13"
        android:layout_alignStart="@+id/textView13" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PROCEED"
        android:id="@+id/save_next_year2015"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
